<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:angle="270" android:endColor="#CDAD00" android:startColor="#CD853F" android:type="linear" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:angle="270" android:endColor="#595959" android:startColor="#000000" android:type="linear" />
        </shape>
    </item>
</selector>